{"ast":null,"code":"import Popup from \"@/components/PopupComponent.vue\";\nexport default {\n  name: \"FormComponent\",\n  components: {\n    Popup\n  },\n  methods: {\n    async onSubmit() {\n      const newInformation = {\n        vehicle: this.vehicle,\n        city: this.city\n      };\n      if (newInformation.vehicle === \"\") {\n        this.headerMsg = \"ERROR\";\n        this.bodyMsg = \"Add delivery vehicle information!\";\n      } else if (newInformation.city === \"\") {\n        this.headerMsg = \"ERROR\";\n        this.bodyMsg = \"Add delivery city information!\";\n      } else {\n        let response = await fetch(\"http://localhost:8080/api/deliveryFee?station=\" + newInformation.city + \"&vehicle=\" + newInformation.vehicle);\n        response = await response.text();\n        if (!isNaN(response) && !isNaN(parseFloat(response))) {\n          this.headerMsg = \"RESULT\";\n          this.bodyMsg = \"Delivery fee will be \" + response + \" €.\";\n        } else {\n          this.headerMsg = \"NOTIFICATION\";\n          this.bodyMsg = response;\n        }\n      }\n      this.showPopup = true;\n    }\n  }\n};","map":{"version":3,"names":["Popup","name","components","methods","onSubmit","newInformation","vehicle","city","headerMsg","bodyMsg","response","fetch","text","isNaN","parseFloat","showPopup"],"sources":["C:\\Users\\37253\\Desktop\\Fujitsu-test-assignment\\frontend\\src\\components\\FormComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Find Delivery Fee</h2>\r\n    <Popup :header-msg=\"this.headerMsg\" :body-msg=\"this.bodyMsg\" v-if=\"showPopup\" @close-popup=\"showPopup=false\"></Popup>\r\n    <form @submit.prevent=\"onSubmit\" class=\"form-container\">\r\n      <div>\r\n        <input type=\"radio\" v-model=\"vehicle\" value=\"Car\" name=\"car\">\r\n        <label for=\"car\">CAR</label><br>\r\n        <input type=\"radio\" v-model=\"vehicle\" value=\"Bike\" name=\"bike\">\r\n        <label for=\"bike\">BIKE</label><br>\r\n        <input type=\"radio\" v-model=\"vehicle\" value=\"Scooter\" name=\"scooter\">\r\n        <label for=\"scooter\">SCOOTER</label>\r\n      </div>\r\n      <br>\r\n      <div>\r\n        <select v-model=\"city\">\r\n          <option value=\"\">--Choose a city--</option>\r\n          <option value=\"Tallinn-Harku\">Tallinn</option>\r\n          <option value=\"Tartu-Tõravere\">Tartu</option>\r\n          <option value=\"Pärnu\">Pärnu</option>\r\n        </select>\r\n      </div>\r\n      <button type=\"submit\">CALCULATE</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Popup from \"@/components/PopupComponent.vue\";\r\n\r\nexport default {\r\n  name: \"FormComponent\",\r\n  components: {\r\n    Popup\r\n  },\r\n  methods: {\r\n    async onSubmit() {\r\n      const newInformation = {\r\n        vehicle: this.vehicle,\r\n        city: this.city,\r\n      }\r\n      if (newInformation.vehicle === \"\") {\r\n        this.headerMsg = \"ERROR\";\r\n        this.bodyMsg = \"Add delivery vehicle information!\"\r\n      } else if (newInformation.city === \"\") {\r\n        this.headerMsg = \"ERROR\";\r\n        this.bodyMsg = \"Add delivery city information!\"\r\n      } else {\r\n        let response = await fetch(\"http://localhost:8080/api/deliveryFee?station=\" + newInformation.city + \"&vehicle=\" + newInformation.vehicle);\r\n        response = await response.text();\r\n        if (!isNaN(response) && !isNaN(parseFloat(response))) {\r\n          this.headerMsg = \"RESULT\"\r\n          this.bodyMsg = \"Delivery fee will be \" + response + \" €.\"\r\n        } else {\r\n          this.headerMsg = \"NOTIFICATION\"\r\n          this.bodyMsg = response;\r\n        }\r\n      }\r\n      this.showPopup = true;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  text-align: center;\r\n  width: 60%;\r\n  min-height: 500px;\r\n  padding: 10px;\r\n  margin: 20px 10px 20px 10px;\r\n  border-radius: 5px;\r\n  background-color: var(--vanilla);\r\n}\r\n.form-container{\r\n  margin: 5px 20px 20px 20px;\r\n}\r\n/* Inspiration for button taken from here - https://copy-paste-css.com/ */\r\nbutton {\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n  line-height: 1;\r\n  border-radius: 300px;\r\n  border-color: var(--buff);\r\n  min-width: 160px;\r\n  white-space: normal;\r\n  font-weight: 550;\r\n  text-align: center;\r\n  padding: 16px 14px 18px;\r\n  margin: 20px;\r\n  color: white;\r\n  background-color: var(--buff);\r\n  height: 48px;\r\n}\r\nbutton:hover{\r\n  background-color: var(--old-rose);\r\n}\r\nselect {\r\n  background-color: var(--beige);\r\n  border: 6px solid transparent;\r\n}\r\ninput[type=radio] {\r\n  accent-color: var(--old-rose);\r\n}\r\n@media screen and (max-width: 480px) {\r\n  button {\r\n    font-size: 12px;\r\n    min-width: 90px;\r\n    margin: 10px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,OAAOA,KAAI,MAAO,iCAAiC;AAEnD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMC,cAAa,GAAI;QACrBC,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,IAAI,EAAE,IAAI,CAACA;MACb;MACA,IAAIF,cAAc,CAACC,OAAM,KAAM,EAAE,EAAE;QACjC,IAAI,CAACE,SAAQ,GAAI,OAAO;QACxB,IAAI,CAACC,OAAM,GAAI,mCAAkC;MACnD,OAAO,IAAIJ,cAAc,CAACE,IAAG,KAAM,EAAE,EAAE;QACrC,IAAI,CAACC,SAAQ,GAAI,OAAO;QACxB,IAAI,CAACC,OAAM,GAAI,gCAA+B;MAChD,OAAO;QACL,IAAIC,QAAO,GAAI,MAAMC,KAAK,CAAC,gDAA+C,GAAIN,cAAc,CAACE,IAAG,GAAI,WAAU,GAAIF,cAAc,CAACC,OAAO,CAAC;QACzII,QAAO,GAAI,MAAMA,QAAQ,CAACE,IAAI,EAAE;QAChC,IAAI,CAACC,KAAK,CAACH,QAAQ,KAAK,CAACG,KAAK,CAACC,UAAU,CAACJ,QAAQ,CAAC,CAAC,EAAE;UACpD,IAAI,CAACF,SAAQ,GAAI,QAAO;UACxB,IAAI,CAACC,OAAM,GAAI,uBAAsB,GAAIC,QAAO,GAAI,KAAI;QAC1D,OAAO;UACL,IAAI,CAACF,SAAQ,GAAI,cAAa;UAC9B,IAAI,CAACC,OAAM,GAAIC,QAAQ;QACzB;MACF;MACA,IAAI,CAACK,SAAQ,GAAI,IAAI;IACvB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}